cutcode

transferPlaybackHere() {
  const { deviceId, token } = this.state;
  console.log(deviceId)
  fetch("https://api.spotify.com/v1/me/player", {
    method: "PUT",
    headers: {
      authorization: `Bearer ${token}`,
      'Content-Type': "application/json",
    },
    body: JSON.stringify({
      "device_ids": [ deviceId ],
      "play": true,
    }),
  });
}
